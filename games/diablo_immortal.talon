app.name: DiabloImmortal.exe
-

inventory:
    mouse_move(1650, 105)
    mouse_click()
close inventory:
    mouse_move(1620, 100)
    mouse_click()
[move] left:
    key(a)
[move] right:
    key(d)
[move] forward:
    key(w)
[move] back:
    key(s)
ability 1:
    key(1)
ability 2:
    key(2)
ability 3:
    key(3)
ability 4:
    key(q)
