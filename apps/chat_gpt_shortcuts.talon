app: msedge.exe
title: /chatgpt|chat gpt/i
-

new chat: 
    key(ctrl-shift-o)

focus input: 
    key(shift-esc)

copy code block: 
    key(ctrl-shift-;)

copy response: 
    key(ctrl-shift-c)

set instructions: 
    key(ctrl-shift-i)

toggle sidebar: 
    key(ctrl-shift-s)

delete chat: 
    key(ctrl-shift-backspace)

show shortcuts: 
    key(ctrl-/)
