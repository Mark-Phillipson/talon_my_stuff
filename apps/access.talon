app.exe: msaccess.exe
-
datasheet view:
    key(alt-h)
    sleep(100ms)
    key(w)
    sleep(100ms)
    key(h)
design view:
    key(alt-h)
    sleep(100ms)
    key(w)
    sleep(100ms)
    key(d)
step into:
    key(f8)
step over:
    key(shift-f8)
step out:
    key(ctrl-shift-f8)
run to cursor:
    key(ctrl-f8)
toggle breakpoint:
    key(f9)
clear all breakpoints:
    key(ctrl-shift-f9)
reset:
    key(ctrl-break)
compile:
    key(alt-d)
    sleep(100ms)
    key(c)
immediate window: key(ctrl-g)